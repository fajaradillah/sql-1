def bubble_sort(arr):
    n = len(arr)
    for i in range(n):
        for j in range(0, n-i-1):
            if arr[j] > arr[j+1]:
                arr[j], arr[j+1] = arr[j+1], arr[j]
    return arr

# Input list
numbers = [21, 400, 8, -3, 77, 99, -16, 55, 111, -36, 28]
# Sorting the list
sorted_numbers = bubble_sort(numbers)
print(sorted_numbers)
